@model Phone

<div class="form_row">
    <label for="">
        Наименование
        <input asp-for="Name" type="text">
    </label>
</div>

<div class="form_row">
    <label for="">
        Компания
        <input asp-for="Company" type="text">
    </label>
</div>

<div class="form_row">
    <label for="">
        Стоимость
        <input asp-for="Price" type="text">
    </label>
</div>

<div class="form_row">
    <label for="">
        Изображение
        <input asp-for="Avatar" type="text">
    </label>
</div>

<div class="form_row">
    <label for="">
        Описание
        <input asp-for="Description" type="text">
    </label>
</div>